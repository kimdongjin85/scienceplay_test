<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.msk.dao.UsersDao">

	<select id="getList" resultType="users">
		SELECT
			CUSTOMER_IDX,	
			ID,
			PASSWORD  
		FROM
			USERS 		
	</select>
	
